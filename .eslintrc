{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "plugins": ["jsx-a11y"],
    "extends": ["eslint:recommended", "plugin:jsx-a11y/recommended", "plugin:prettier/recommended"],
    "rules": {
      "strict": ["error", "never"]
    },
    "env": {
      "browser": true
    }
  }
}
